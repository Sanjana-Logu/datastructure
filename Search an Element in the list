class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

head = Node("A")
head.next = Node("B")
head.next.next = Node("C")

def search(head, key):
    current = head
    position = 1
    while current:
        if current.data == key:
            print(f"Element '{key}' found at position {position}")
            return
        current = current.next
        position += 1
    print(f"Element '{key}' not found in the list.")

search(head, "B")
search(head, "X")
